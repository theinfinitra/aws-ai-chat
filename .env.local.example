# AWS AI Chat - Environment Configuration
# Copy this file to .env.local and update the values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=your-aws-profile
BEDROCK_MODEL_ID=amazon.nova-pro-v1:0

# API Configuration (will be set after backend deployment)
NEXT_PUBLIC_API_URL=https://your-api-id.execute-api.us-east-1.amazonaws.com/dev

# WebSocket Configuration (optional - will fallback to HTTP streaming if not provided)
NEXT_PUBLIC_WEBSOCKET_URL=wss://your-websocket-id.execute-api.us-east-1.amazonaws.com/dev

# Development settings
NODE_ENV=development
